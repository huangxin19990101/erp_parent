<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.huangxin.entity.Inventory" table="Inventory">
			<id name="uuid">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
			
				<param name="sequence_name">inventory_seq</param>
			</generator>
		</id>

		<property name="goodsuuid"></property>
<property name="storeuuid"></property>
<property name="num"></property>
<property name="type"></property>
<property name="createtime"></property>
<property name="checktime"></property>
<property name="creater"></property>
<property name="checker"></property>
<property name="state"></property>
<property name="remark"></property>

	</class>
</hibernate-mapping>
